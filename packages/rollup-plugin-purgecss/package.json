{
  "name": "rollup-plugin-purgecss",
  "version": "4.1.3",
  "description": "Rollup plugin for purgecss",
  "main": "lib/rollup-plugin-purgecss.js",
  "module": "./lib/rollup-plugin-purgecss.es.js",
  "jsnext:main": "./lib/rollup-plugin-purgecss.es.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "scripts": {
    "api-extractor": "api-extractor run && rm -rf lib/.temp",
    "build": "npm run clean && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript && npm run api-extractor",
    "clean": "rm -rf ./lib",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FullHuman/purgecss.git"
  },
  "keywords": [
    "rollup-plugin",
    "purgecss",
    "remove",
    "unused",
    "css"
  ],
  "author": "Ffloriel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FullHuman/purgecss/issues"
  },
  "homepage": "https://purgecss.com",
  "dependencies": {
    "purgecss": "^4.1.3",
    "rollup-pluginutils": "^2.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
